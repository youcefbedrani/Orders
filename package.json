{
  "name": "pixwarm",
  "version": "1.0.0",
  "description": "PixWarm - Premium Pixel Warming Automation Tool",
  "scripts": {
    "dev": "next dev",
    "build": "next build && mkdir -p .next/standalone/public && cp -r public/* .next/standalone/public/ || true && mkdir -p .next/standalone/.next/static && cp -r .next/static/* .next/standalone/.next/static/ || true && cp -r prisma .next/standalone/prisma",
    "start": "node .next/standalone/server.js",
    "postinstall": "prisma generate",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^7.0.9",
    "bcryptjs": "^3.0.3",
    "next": "^14.0.0",
    "nodemailer": "^7.0.13",
    "puppeteer": "^21.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
